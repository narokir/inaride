<div class="page-header">
<h1>Users</h1>
</div>

<table class="table table-striped table-bordered">
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Login Provider</th>
    <th>Profile Image</th>
    <th>uid</th>
    <th>id</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= link_to user.name, user %></td>
    <td><%= user.email %></td>
    <td><%= user.provider %></td>
    <td><%= user.img_url %></td>
    <td><%= user.uid %></td>
    <td><%= user.id %></td>
    <td>
          <div class="btn-group">
    <a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
    Action
    <span class="caret"></span>
    </a>
    <ul class="dropdown-menu">
      <li><%= link_to 'Show', user %></li>
      <li><%= link_to 'Edit', edit_user_path(user) %></li>
      <li><%= link_to 'Destroy', user, confirm: 'Are you sure?', method: :delete %></li>
    </ul>
    </div>
    </td>

  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path, class:"btn btn-large btn-primary" %>




